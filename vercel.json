{
  "version": 2,
  "installCommand": "npm install --production=false",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "s-maxage=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
